package com.itwill.sida.dto;

public class Card {
	
	private int c_no;
	private String c_m_emil;
	private String c_name;
	private String c_company;
	private String c_keyword;
	
	public Card() {}

	public Card(int c_no, String c_m_id, String c_name, String c_company, String c_keyword) {
		super();
		this.c_no = c_no;
		this.c_m_emil = c_m_id;
		this.c_name = c_name;
		this.c_company = c_company;
		this.c_keyword = c_keyword;
	}

	public int getC_no() {
		return c_no;
	}

	public void setC_no(int c_no) {
		this.c_no = c_no;
	}

	public String getC_m_id() {
		return c_m_emil;
	}

	public void setC_m_id(String c_m_id) {
		this.c_m_emil = c_m_id;
	}

	public String getC_name() {
		return c_name;
	}

	public void setC_name(String c_name) {
		this.c_name = c_name;
	}

	public String getC_company() {
		return c_company;
	}

	public void setC_company(String c_company) {
		this.c_company = c_company;
	}

	public String getC_keyword() {
		return c_keyword;
	}

	public void setC_keyword(String c_keyword) {
		this.c_keyword = c_keyword;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "Card [c_no=" + c_no + ", c_m_emil=" + c_m_emil + ", c_name=" + c_name + ", c_company=" + c_company + ", c_keyword=" + c_keyword + "]";
	}

}
